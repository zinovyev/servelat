{
    "name": "zinovyev/servelat",
    "description": "Servelat is a framework wich allowes you to do things async with the PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Zinovyev",
            "email": "vanyazin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "pimple/pimple": "~3.0",
        "monolog/monolog": "^1.17",
        "symfony/event-dispatcher": "^2.7",
        "symfony/options-resolver": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Servelat\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Servelat\\Tests\\": "tests"
        }
    }
}
